pulumi_content: |-
  #!/bin/sh

  aws --version
  go version
  pulumi stack init dev
  pulumi config set aws:profile platform
  pulumi config set aws:region us-west-1
  echo $AWS_REGION
  cat ~/.aws/credentials
  pulumi up --yes
  pulumi stack output kubeconfig --show-secrets >kubeconfig.json
  #KUBECONFIG=./kubeconfig.json kubectl get nodes
  echo "kubeconfig--"
  pulumi stack output --show-secrets
  #echo $KUBECONFIG 
  env
pulumi_git_path: aws-py-eks
pulumi_uninstall: |-
  #!/bin/sh

  pulumi destroy --yes
  pulumi stack rm --yes
