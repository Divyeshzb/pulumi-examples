pulumi_content: |-
  #!/bin/sh

  echo "Kubeconfig path is: $KUBECONFIG"
  pulumi stack select dev
  pulumi config set aws:region us-west-1
  echo $AWS_REGION
  cat /home/ubuntu/.aws/credentials
  cat ~/.aws/credentials
  pulumi up --yes
  pulumi stack output kubeconfig --show-secrets >$KUBECONFIG
  echo "kubeconfig--"
  pulumi stack output --show-secrets
  #echo $KUBECONFIG
  env
pulumi_git_path: aws-go-eks
pulumi_uninstall: |-
  #!/bin/sh

  pulumi destroy --yes
  pulumi stack rm --yes
